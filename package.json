{
  "name": "projecty",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --prod",
    "start-dev": "npm run copy-ds-files && ng serve",
    "build": "npm run copy-ds-files && ng build --prod --base-href /projecty",
    "build-dev": "ng build --base-href /PoseidonNext/projecty/",
    "copy-ds-files": "copyfiles --up 2 ..\\..\\node_modules\\@kognifai\\design-system\\**\\* .",
    "update": "copy /Y package.publish.json dist\\package.json && copy /Y app.manifest.prod.json dist\\app.manifest.json && copy /Y src\\config.prod.json dist\\config.json",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "author": "Kongsberg Digital",
  "publishConfig": {
    "directory": "dist"
  },
